{
  "expo": {
    "name": "billburst-rn",
    "slug": "billburst-rn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/bb-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFCD2A"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFCD2A"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
